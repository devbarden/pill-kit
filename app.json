{
	"expo": {
		"name": "Pill Kit",
		"slug": "pill-kit",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/logo.png",
		"userInterfaceStyle": "light",
		"platforms": ["ios", "android"],
		"splash": {
			"image": "./assets/logo-without-bg.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": false,
			"bundleIdentifier": "com.devbarden.pillkit"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/logo.png",
				"backgroundColor": "#ffffff"
			},
			"package": "com.devbarden.pillkit"
		},
		"plugins": [
			["expo-localization"],
			[
				"expo-notifications",
				{
					"icon": "./assets/logo-without-bg.png",
					"color": "#A4161A",
					"backgroundColor": "#ffffff"
				}
			]
		],
		"extra": {
			"eas": {
				"projectId": "50a60acb-e9e2-4f35-aea8-4b45c8ddbb11"
			}
		}
	}
}
